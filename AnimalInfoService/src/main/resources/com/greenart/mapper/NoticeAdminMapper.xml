<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.greenart.mapper.NoticeAdminMapper">
    <select id="selectAdminStatus" resultType="com.greenart.vo.MemberInfoVO">
        select
            seq,name,password,id,email,gen,birth,address,address_detail,phone,status
        from
            member_info
        where status = #{status}
    </select>
</mapper>